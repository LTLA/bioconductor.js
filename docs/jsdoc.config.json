{
    "opts": {
        "template": "node_modules/docdash"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "docdash": {
        "static": true,
        "sort": true,
        "search": true,
        "collapse": true,
        "typedefs": true,
        "removeQuotes": "none",
        "scripts": [],
        "menu": {
            "Code Repo": {
                "href": "https://github.com/LTLA/bioconductor.js",
                "target": "_blank",
                "class": "menu-item",
                "id": "repository"
            }
        }
    },
    "templates": {
        "default": {
            "useLongnameInNav": true
        }
    }
}
